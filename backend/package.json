    "bcrypt": "^5.0.1",
    "express-session": "^1.17.2",
    "json-server": "^0.16.3",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express-session": "^1.17.5",
    "@types/json-server": "^0.14.4",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.6.4",
    "dotenv": "^16.0.1",
    "nodemon": "^2.0.19",
    "prisma": "^4.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
